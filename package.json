{
  "name": "juanse-finance-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node build/app.js",
    "dev": "ts-node-dev src/app.ts",
    "build": "npm install && tsc",
    "lint": "ts-standard",
    "migrations": "sequelize-cli db:migrate",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:undo": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:delete": "sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanespinola05/t-finance-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanespinola05/t-finance-api/issues"
  },
  "homepage": "https://github.com/juanespinola05/t-finance-api#readme",
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "bcrypt": "^5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "jsonwebtoken": "8.5.1",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.25.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "morgan": "1.10.0",
    "nodemon": "2.0.20",
    "sequelize-cli": "6.5.2",
    "standard": "17.0.0",
    "ts-node-dev": "2.0.0",
    "ts-standard": "12.0.1",
    "typescript": "4.8.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/eslintrc.json"
    ]
  }
}
